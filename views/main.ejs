

<body>
    <form action="/login" method="get">
        <div>
            <input type="submit" value="Войти"/>
        </div>
    </form>
    <form action="/signup" method="get">
        <div>
            <input type="submit" value="Зарегистрироваться"/>
        </div>
    </form>
    <form action="/account" method="get">
        <div>
            <input type="submit" value="Пользователь"/>
        </div>
    </form>
    <form action="/dialog/authorize" method="get">
        <div>
            <input type="submit" value="Авторизовать приложение"/>
        </div>
    </form>
    <form action="/api/magazines" method="get">
        <div>
            <input type="submit" value="Все журналы"/>
        </div>
    </form>

    <form>
        <div>
            <label>Найти журнал по имени:</label>
            <input type="text" name="name"/>
            <input type="submit" value="Найти" onclick="loadDoc()"/>
        </div>
    </form>

    <script>
        function loadDoc() {
            var xhttp = new XMLHttpRequest();
            xhttp.open("GET", "dialog/authorize?response_type=code&client_id=first&redirect_uri=/", true);
            xhttp.send();
        }
    </script>

    <form action="/test/magazine?_method=PUT" method="post">
        <div>
            <label>Изменить описание журнала</label>
        </div>
        <div>
            <label>Название журнала</label>
            <input type="text" name="name"/>
        </div>
        <div>
            <label>Ввести описание</label>
            <input type="text" name="description"/>
        </div>
            <input type="submit" value="Изменить"/>
        </div>
    </form>
    <form action="/test/magazine?_method=DELETE" method="post">
        <div>
            <label>Удалить журнал по имени:</label>
            <input type="text" name="name"/>
            <input type="submit" value="Удалить"/>
        </div>
    </form>
    <form action="/test/publishingHouse" method="post">
        <div>
            <label>Добавить новый журнал в свой издательский дом.</label>
            <label>Новое название журнала:</label>
            <input type="text" name="nameMagazine"/>
            <input type="submit" value="Добавить"/>
        </div>
    </form>
</body>