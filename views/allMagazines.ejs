<form action="/main" method="get">
    <div>
        <input type="submit" value="На главную"/>
    </div>
</form>
<ul class="list-group">
  <% magazines.forEach( function( magazine ) { %>
      <li class="list-group-item">
        <p>Название: <%= magazine.name %>
           ,    Издательский дом: <%= magazine.pubHouseName %>
           ,    Описание: <%= magazine.description %>
        </p>
      </li>
  <% }) %>
</ul>

<% if (pageCount > 1) { %>
<ul class="pagination">
  <% if (currentPage > 1) { %>
      <li><a href="/api/magazines/?page=1">&laquo;</a></li>
  <% } %>
  <% var i = 1;
  if (currentPage > 5) {
  i = +currentPage - 4;
  } %>
  <% if (i !== 1) { %>
      <li class="disabled"><a href="/api/magazines#">...</a></li>
  <% } %>
  <% for (i; i<=pageCount; i++) { %>
    <% if (currentPage == i) { %>
        <li class="active"><span><%= i %> <span class="sr-only">(текущая)</span></span></li>
    <% } else { %>
        <li><a href="/api/magazines/?page=<%= i %>"><%= i %></a></li>
    <% } %>
    <% if (i == (+currentPage + 4)) { %>
        <li class="disabled"><a href="/api/magazines#">...</a></li>
    <% break; } %>
  <% } %>
  <% if (currentPage != pageCount) { %>
      <li><a href="/api/magazines/?page=<%= pageCount %>">&raquo;</a></li>
  <% } %>
</ul>
<% } %>